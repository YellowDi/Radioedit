					["custom"] = "local config = aura_env.config\n\nlocal allTypes = {\n    \"INTERRUPT\",\n    \"HARDCC\", \"STHARDCC\",\n    \"SOFTCC\", \"STSOFTCC\",\n    \"DISPEL\", \"DEFMDISPEL\",\n    \"EXTERNAL\", \"HEALING\",\n    \"UTILITY\",\n    \"PERSONAL\", \"IMMUNITY\",\n    \"DAMAGE\",\n    \"TANK\"\n}\n\naura_env.types = {}\nfor _,type in ipairs(allTypes) do\n    if config[\"enabled\"..type] then\n        aura_env.types[type] = true\n    end\nend\n\naura_env.computeSortIndex = function(type, spellID, member)\n    local index = \"\"\n    \n    for v = 1,4 do\n        local var = config[\"sortVar\"..v]\n        if var == 2 then     -- Type Priority\n            index = index..((\"%02d\"):format(config[\"priority\"..type]))\n        elseif var == 3 then -- Spell ID\n            index = index..((\"%06d\"):format(spellID))\n        elseif var == 4 then -- Member Class\n            index = index..((\"%02d\"):format(member.classID))\n        elseif var == 5 then -- Member Name\n            index = index..((\"%-12s\"):format(member.name))\n        end\n    end\n    \n    return index\nend",
